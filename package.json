{
  "author": "brentlok",
  "name": "@codemaskinc/create-app-template",
  "version": "1.5.1",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/codemaskinc/create-app-template",
  "keywords": [
    "react",
    "react-native",
    "astro",
    "nest-js"
  ],
  "bin": "dist/cli.js",
  "type": "module",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "prepublishOnly": "yarn install && yarn run build",
    "build": "tsc",
    "dev": "tsc --watch",
    "tsc": "tsc"
  },
  "files": [
    "dist",
    "template"
  ],
  "dependencies": {
    "execa": "8.0.1",
    "fs-extra": "11.1.1",
    "ink": "4.4.1",
    "meow": "11.0.0",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "3.0.1",
    "@types/react": "18.2.31",
    "@types/fs-extra": "11.0.3",
    "@vdemedes/prettier-config": "2.0.1",
    "ava": "5.3.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "eslint": "8.52.0",
    "prettier": "3.0.3",
    "typescript": "5.2.2",
    "xo": "0.56.0"
  },
  "ava": {
    "extensions": {
      "ts": "module",
      "tsx": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "xo": {
    "extends": "xo-react",
    "prettier": true,
    "rules": {
      "react/prop-types": "off"
    }
  },
  "prettier": "@vdemedes/prettier-config"
}
