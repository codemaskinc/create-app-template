{
	"name": "create-codemask-app",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"ink": "4.4.1",
		"meow": "11.0.0",
		"react": "18.2.0",
        "fs-extra": "11.1.1",
        "execa": "8.0.1",
        "npm-check-updates": "16.14.5"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "3.0.1",
		"@types/react": "18.2.27",
        "@types/fs-extra": "11.0.2",
		"@vdemedes/prettier-config": "2.0.1",
		"ava": "5.3.1",
		"eslint-plugin-react": "7.33.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"prettier": "3.0.3",
		"typescript": "5.2.2",
		"xo": "0.56.0"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config"
}
